{
  "name": "piezasyaya-backend",
  "version": "1.0.0",
  "description": "Backend reactivo para ecommerce de repuestos de veh√≠culos - PiezasYA",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node --transpile-only ./src/index.ts src/index.ts",
    "dev:network": "node start-with-specific-ip.js",
    "dev:simple": "ts-node --transpile-only ./src/index.ts",
    "dev:windows": "ts-node --transpile-only ./src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "check-all": "npm run type-check && npm run lint",
    "seed": "ts-node src/scripts/seedData.ts",
    "import-sheets": "ts-node src/scripts/importFromGoogleSheets.ts",
    "import-csv": "ts-node src/scripts/importFromCSV.ts",
    "import-brands": "ts-node src/scripts/importBrands.ts",
    "import-subcategories": "ts-node src/scripts/importSubcategories.ts",
    "check-port": "node check-ports.js",
    "free-port": "node check-ports.js 5000",
    "generate-products": "ts-node src/scripts/generateTestProducts.ts",
    "setup:vehicle-types": "ts-node src/scripts/setupVehicleTypes.ts",
    "seed:venezuela-brands": "ts-node src/scripts/seedVenezuelaBrands.ts"
  },
  "keywords": [
    "ecommerce",
    "repuestos",
    "piezasyaya",
    "reactivo",
    "express",
    "mongodb"
  ],
  "author": "PiezasYA Team",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@types/express-session": "^1.18.2",
    "@types/google.maps": "^3.58.1",
    "@types/nodemailer": "^7.0.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "argon2": "^0.44.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0-rc.12",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "googleapis": "^154.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.0",
    "mongoose-paginate-v2": "^1.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.5.4",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "tesseract.js": "^6.0.1",
    "web-push": "^3.6.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2"
  }
}

{
  "name": "piezasyaya-ecommerce",
  "version": "1.4.0",
  "description": "Ecommerce multiplataforma para repuestos de veh√≠culos - PiezasYA v1.4.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend:network\"",
    "dev:frontend": "vite --port 3000",
    "dev:backend": "cd backend && npm run dev",
    "dev:backend:network": "cd backend && npm run dev:network",
    "dev:fast": "concurrently \"npm run dev:frontend\" \"npm run dev:backend:network\" --kill-others",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "node --max-old-space-size=4096 node_modules/vite/bin/vite.js build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm install && cd backend && npm install",
    "start": "cd backend && npm start",
    "test:servers": "node test-servers.js",
    "kill": "taskkill /F /IM node.exe"
  },
  "keywords": [
    "ecommerce",
    "repuestos",
    "piezasyaya",
    "react",
    "express",
    "mongodb"
  ],
  "author": "Juan Hernandez <jcarweb.designer@gmail.com> (LDJ Digital Solutions)",
  "contributors": [
    "PiezasYA Development Team"
  ],
  "license": "MIT",

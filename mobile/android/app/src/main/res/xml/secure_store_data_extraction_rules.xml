<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude secure store data from cloud backup -->
        <exclude domain="sharedpref" path="SecureStore"/>
        <exclude domain="database" path="SecureStore"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude secure store data from device transfer -->
        <exclude domain="sharedpref" path="SecureStore"/>
        <exclude domain="database" path="SecureStore"/>
    </device-transfer>
</data-extraction-rules>
